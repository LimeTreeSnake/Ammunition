<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!--==================== Large ======================== -->
	<ThingDef ParentName="KitBase">
		<defName>Apparel_AmmoPackPrimitiveLarge</defName>
		<label>large primitive kit</label>
		<description>A large pack for carrying primitive ammunition. It has enough space for 60 ammo.</description>    
		<graphicData>
			<texPath>MagPrimitive/AmmoPackPrimitiveLarge</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
		</graphicData>
		<techLevel>Neolithic</techLevel>
		<statBases>
			<Mass>6</Mass>
			<WorkToMake>2500</WorkToMake>
			<Flammability>0.6</Flammability>
			<EquipDelay>2</EquipDelay>
		</statBases>		 
		<costStuffCount>40</costStuffCount>
		<stuffCategories>
			<li>Fabric</li>
			<li>Leathery</li>
		</stuffCategories>   
		<tradeability>All</tradeability>
		<thingCategories>
			<li>AmmoPacks</li>
		</thingCategories>		
		<apparel>
			<bodyPartGroups>
				<li>Waist</li>
			</bodyPartGroups>
			<layers>
				<li>Kit</li>
			</layers>
			<tags>
				<li>Neolithic</li>
			</tags>
		</apparel>
		<recipeMaker>
			<recipeUsers>
				<li>ElectricTailoringBench</li>
				<li>HandTailoringBench</li>
				<li>CraftingSpot</li>
			</recipeUsers>
		</recipeMaker>  
		<comps>			
			<li Class="Ammunition.Comps.CompProps_Magazine">
				<AmmoCapacity>60</AmmoCapacity>
				<AmmoDef>AmmoPrimitive</AmmoDef>
				<Icon>MagPrimitive/AmmoPackPrimitiveLarge</Icon>
			</li>
		</comps>    	
	</ThingDef>

