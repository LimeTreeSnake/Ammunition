<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!--==================== Massive ======================== -->
	<ThingDef ParentName="KitBase">
		<defName>Apparel_AmmoPackPrimitiveMassive</defName>
		<label>massive primitive kit</label>
		<description>A massive pack for carrying primitive ammunition. It has enough space for 80 ammo.</description>    
		<graphicData>
			<texPath>MagPrimitive/AmmoPackPrimitiveMassive</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
		</graphicData>
		<techLevel>Neolithic</techLevel>
		<statBases>
			<Mass>8</Mass>
			<WorkToMake>4000</WorkToMake>
			<Flammability>0.6</Flammability>
			<EquipDelay>2</EquipDelay>
		</statBases>		 
		<costStuffCount>60</costStuffCount>
		<stuffCategories>
			<li>Fabric</li>
			<li>Leathery</li>
		</stuffCategories>   
		<tradeability>All</tradeability>
		<thingCategories>
			<li>AmmoPacks</li>
		</thingCategories>		
		<apparel>
			<bodyPartGroups>
				<li>Torso</li>
			</bodyPartGroups>
			<layers>
				<li>Kit</li>
			</layers>
			<tags>
				<li>Neolithic</li>
			</tags>
		</apparel>
		<recipeMaker>
			<recipeUsers>
				<li>ElectricTailoringBench</li>
				<li>HandTailoringBench</li>
				<li>CraftingSpot</li>
			</recipeUsers>
		</recipeMaker>  
		<comps>			
			<li Class="Ammunition.Comps.CompProps_Magazine">
				<AmmoCapacity>80</AmmoCapacity>
				<AmmoDef>AmmoPrimitive</AmmoDef>
				<Icon>MagPrimitive/AmmoPackPrimitiveMassive</Icon>
			</li>
		</comps>    
	</ThingDef>

