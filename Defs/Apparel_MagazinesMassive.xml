<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!--==================== Massive ======================== -->
	<ThingDef ParentName="ApparelMakeableBase">
		<defName>Apparel_AmmoPackPrimitiveMassive</defName>
		<label>massive primitive pack</label>
		<description>A massive pack for carrying primitive ammunition.</description>    
		<thingClass>Ammunition.Things.Magazine</thingClass>
		<graphicData>
			<texPath>MagPrimitive/AmmoPackPrimitiveMassive</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<techLevel>Neolithic</techLevel>
		<statBases>
			<Mass>10</Mass>
			<WorkToMake>4000</WorkToMake>
			<Flammability>0.6</Flammability>
			<EquipDelay>2</EquipDelay>
		</statBases>		 
		<costStuffCount>100</costStuffCount>
		<stuffCategories>
			<li>Fabric</li>
			<li>Leathery</li>
		</stuffCategories>   
		<tradeability>Sellable</tradeability>
		<thingCategories>
			<li>Apparel</li>
		</thingCategories>		
		<apparel>
			<bodyPartGroups>
				<li>Waist</li>
			</bodyPartGroups>
			<layers>
				<li>Shell</li>
			</layers>
			<tags>
				<li>Neolithic</li>
			</tags>
		</apparel>
		<recipeMaker>
			<recipeUsers>
				<li>ElectricTailoringBench</li>
				<li>HandTailoringBench</li>
				<li>CraftingSpot</li>
			</recipeUsers>
		</recipeMaker>  
		<comps>			
			<li Class="Ammunition.Comps.CompProps_Magazine">
				<AmmoCapacity>64</AmmoCapacity>
				<AmmoDef>AmmoPrimitive</AmmoDef>
				<Icon>MagPrimitive/AmmoPackPrimitiveMassive</Icon>
			</li>
		</comps>    
	</ThingDef>

