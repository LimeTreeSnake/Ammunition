<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!--==================== Medium ======================== -->
	<ThingDef ParentName="PackBase">
		<defName>Apparel_AmmoPackPrimitiveMedium</defName>
		<label>medium primitive pack</label>
		<description>A medium pack for carrying primitive ammunition. It has enough space for 40 ammo.</description>    
		<graphicData>
			<texPath>MagPrimitive/AmmoPackPrimitiveMedium</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
		</graphicData>
		<techLevel>Neolithic</techLevel>
		<statBases>
			<Mass>4</Mass>
			<WorkToMake>1500</WorkToMake>
			<Flammability>0.6</Flammability>
			<EquipDelay>2</EquipDelay>
		</statBases>		 
		<costStuffCount>25</costStuffCount>
		<stuffCategories>
			<li>Fabric</li>
			<li>Leathery</li>
		</stuffCategories>   
		<tradeability>All</tradeability>
		<thingCategories>
			<li>AmmoPacks</li>
		</thingCategories>		
		<apparel>
			<bodyPartGroups>
				<li>Shoulders</li>
			</bodyPartGroups>
			<layers>
				<li>Pack</li>
			</layers>
			<tags>
				<li>Neolithic</li>
			</tags>
		</apparel>
		<recipeMaker>
			<recipeUsers>
				<li>ElectricTailoringBench</li>
				<li>HandTailoringBench</li>
				<li>CraftingSpot</li>
			</recipeUsers>
		</recipeMaker>  
		<comps>			
			<li Class="Ammunition.Comps.CompProps_Magazine">
				<AmmoCapacity>40</AmmoCapacity>
				<AmmoDef>AmmoPrimitive</AmmoDef>
				<Icon>MagPrimitive/AmmoPackPrimitiveMedium</Icon>
			</li>
		</comps>    
	</ThingDef>

